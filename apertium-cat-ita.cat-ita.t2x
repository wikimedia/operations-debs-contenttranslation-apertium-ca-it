<?xml version="1.0" encoding="ISO-8859-1"?> <!-- -*- nxml -*- -->
<interchunk>
  <section-def-cats>

    <def-cat n="SN">
      <cat-item tags="SN.*"/>
    </def-cat>

    <def-cat n="SA">
      <cat-item tags="SA.*"/>
    </def-cat>

    <def-cat n="un_num">
      <cat-item lemma="uno" tags="SN.*"/>
      <cat-item lemma="un" tags="SA.*"/>
      <cat-item tags="NUM.*"/>
    </def-cat>

    <def-cat n="SN_SA_NUM">
      <cat-item tags="SN.*"/>
      <cat-item tags="SA.*"/>
      <cat-item tags="NUM.*"/>
    </def-cat>

    <def-cat n="top_potser_det"> <!-- topònims marcats a t1x com a susceptibles de necessitar un article -->
      <cat-item lemma="top_potser_det" tags="SN.*"/>
    </def-cat>

    <def-cat n="SAdv">
      <cat-item tags="SAdv"/>
      <cat-item tags="SAdv.*"/>
    </def-cat>

    <def-cat n="SAdv_PrnPro">
      <cat-item tags="SAdv"/>
      <cat-item tags="SAdv.*"/>
      <cat-item tags="SN.prn.pro.*"/>
    </def-cat>

    <def-cat n="SV">
      <cat-item tags="SV.*"/>
    </def-cat>

    <def-cat n="SV_inf">
      <cat-item tags="SV.vblex.inf"/>
      <cat-item tags="SV.vblex.inf.*"/>
    </def-cat>

    <def-cat n="SV_vbser_semblar">
      <cat-item tags="SV.vbser"/>
      <cat-item tags="SV.vbser.*"/>
      <cat-item lemma="parere" tags="SV.vblex.*"/>
      <cat-item lemma="rimanere" tags="SV.vblex.*"/>
      <cat-item lemma="rendere" tags="SV.vblex.*"/>
      <cat-item lemma="diventare" tags="SV.vblex.*"/>
      <cat-item lemma="trovare" tags="SV.vblex.*"/>
