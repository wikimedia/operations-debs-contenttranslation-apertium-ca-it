  <!-- lanciare > llançar, llençar -->
  <rule weight="0.6">
    <match lemma="lanciare" tags="vblex.*"><select lemma="llançar"/></match>
  </rule>

  <!-- sposare > casar-se, casar-se amb -->
  <rule weight="0.6">
    <match lemma="sposare" tags="vblex.*"><select lemma="casar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="sposare" tags="vblex.*"><select lemma="casar# amb"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <or>
      <match tags="np.ant.*"/>
      <match tags="np.cog.*"/>
      <match tags="prn.tn.*"/>
    </or>
  </rule>

  <!-- gettare > llençar, llançar, tirar -->
  <rule weight="0.6">
    <match lemma="gettare" tags="vblex.*"><select lemma="llençar"/></match>
  </rule>

  <!-- ospitare > albergar, hostatjar -->
  <rule weight="0.6">
    <match lemma="ospitare" tags="vblex.*"><select lemma="albergar"/></match>
  </rule>

  <!-- preoccupare > preocupar, encarregar, ocupar -->
  <rule weight="0.6">
    <match lemma="preoccupare" tags="vblex.*"><select lemma="preocupar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="preoccupare" tags="vblex.*"><select lemma="encarregar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="di" tags="pr"/>
    <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
    <match lemma="preoccupare" tags="vblex.*"><select lemma="encarregar"/></match>
    <match tags="prn.enc.*"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="di" tags="pr"/>
    <match tags="vblex.inf"/>
  </rule>

  <!-- giocare > jugar, tenir -->
  <rule weight="0.6">
    <match lemma="giocare" tags="vblex.*"><select lemma="jugar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="giocare" tags="vblex.*"><select lemma="tenir"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
    <match lemma="ruolo" tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="giocare" tags="vblex.*"><select lemma="tenir"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
    <match tags="adj.*"/>
    <match lemma="ruolo" tags="n.*"/>
  </rule>

  <!-- ricoprire > recobrir, cobrir, exercir -->
  <rule weight="0.6">
    <match lemma="ricoprire" tags="vblex.*"><select lemma="cobrir"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ricoprire" tags="vblex.*"><select lemma="cobrir"/></match>
     <repeat from="0" upto="1">
       <match tags="adv"/>
     </repeat>
    <match tags="det.*"/>
    <or>
      <match lemma="ruolo" tags="n.*"/>
      <match lemma="carica" tags="n.*"/>
      <match lemma="incarico" tags="n.*"/>
      <match lemma="funzione" tags="n.*"/>
    </or>
  </rule>

  <!-- gestire > administrar, gestionar -->
  <rule weight="0.6">
    <match lemma="gestire" tags="vblex.*"><select lemma="administrar"/></match>
  </rule>

  <!-- chiarificare > clarificar, aclarir -->
  <rule weight="0.6">
    <match lemma="chiarificare" tags="vblex.*"><select lemma="clarificar"/></match>
  </rule>

  <!-- chiarire > aclarir, clarificar -->
  <rule weight="0.6">
    <match lemma="chiarire" tags="vblex.*"><select lemma="aclarir"/></match>
  </rule>

  <!-- cancellare > esborrar, cancel·lar -->
  <rule weight="0.6">
    <match lemma="cancellare" tags="vblex.*"><select lemma="esborrar"/></match>
  </rule>

  <!-- confinare > confinar, limitar -->
  <rule weight="0.6">
    <match lemma="confinare" tags="vblex.*"><select lemma="confinar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="confinare" tags="vblex.*"><select lemma="limitar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="con" tags="pr"/>
  </rule>
  <rule weight="1.0">
     <match lemma="confinare" tags="vblex.*"><select lemma="limitar"/></match>
     <or>
       <match lemma="a nord"/>
       <match lemma="a sud"/>
       <match lemma="a est"/>
       <match lemma="a ovest"/>
     </or>
  </rule>

  
  <!-- guidare > guiar, conduir -->
  <rule weight="0.6">
     <match lemma="guidare" tags="vblex.*"><select lemma="guiar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="guidare" tags="vblex.*"><select lemma="conduir" tags="vblex.*"/></match>
    <match tags="det.*"/>
    <match lemma="auto" tags="n.*"/>
  </rule>

  <!-- curare > curar, ocupar -->
  <rule weight="0.6">
     <match lemma="curare" tags="vblex.*"><select lemma="curar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="curare" tags="vblex.*"><select lemma="ocupar"/></match>
    <repeat from="0" upto="2">
      <match tags="prn.enc.*"/>
    </repeat>
    <match lemma="di" tags="pr"/>
  </rule>

  <!-- stampare > imprimir, editar, estampar -->
  <rule weight="0.6">
     <match lemma="stampare" tags="vblex.*"><select lemma="imprimir"/></match>
  </rule>

  <!-- segare > serrar, segar  -->
  <rule weight="0.6">
    <match lemma="segare" tags="vblex.*"><select lemma="serrar"/></match>
  </rule>

  <!-- vincere > guanyar, vèncer -->
  <rule weight="0.6">
    <match lemma="vincere" tags="vblex.*"><select lemma="guanyar"/></match>
  </rule>

  <!-- troncare > truncar, troncare -->
  <rule weight="0.6">
    <match lemma="troncare" tags="vblex.*"><select lemma="truncar"/></match>
  </rule>

  <!-- sbandare > derrapar, escorar -->
  <rule weight="0.6">
    <match lemma="sbandare" tags="vblex.*"><select lemma="derrapar"/></match>
  </rule>

  <!-- confrontare > confrontar, comparar, acarar -->
  <rule weight="0.6">
    <match lemma="confrontare" tags="vblex.*"><select lemma="confrontar"/></match>
  </rule>

  <!-- fallire > fracassar (intr), fallar (tr), fallir -->
  <rule weight="0.6">
    <match lemma="fallir" tags="vblex.*"><select lemma="fracassar"/></match>
  </rule>

  <!-- alzare > aixecar, alçar, llevar -->
  <rule weight="0.6">
    <match lemma="alzare" tags="vblex.*"><select lemma="aixecar"/></match>
  </rule>

  <!-- nominare > nomenar, nominar -->
  <rule weight="0.6">
    <match lemma="nominare" tags="vblex.*"><select lemma="nomenar"/></match>
  </rule>

  <!-- disciplinare > disciplinar, reglamentar -->
  <rule weight="0.6">
    <match lemma="disciplinare" tags="vblex.*"><select lemma="disciplinar" tags="vblex.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="disciplinare" tags="vblex.pp.*"><select lemma="reglamentar" tags="vblex.*"/></match>
    <match lemma="da" tags="pr"/>
  </rule>
  
  <!-- stufare > estofar, cansar -->
  <macro name="verb_nopron_pron" p1="stufare" p2="estofar" p3="cansar"/>
  
  <!-- approssimare > apropar, aproximar -->
  <macro name="verb_nopron_pron" p1="approssimare" p2="aproximar" p3="apropar"/>
  
  <!-- avvicinare > apropar, aproximar, assemblar -->
  <!-- <macro name="verb_nopron_pron" p1="avvicinare" p2="apropar" p3="assemblar"/> -->
  <rule weight="0.6">
    <match lemma="avvicinare" tags="vblex.*"><select lemma="apropar"/></match>
  </rule>
  
  <!-- strappare > arrencar, trencar (+ intr: trencar) -->
  <macro name="verb_nopron_pron" p1="strappare" p2="arrencar" p3="trencar"/>

  <!-- scontrare > xocar, encarar -->
  <macro name="verb_nopron_pron" p1="scontrare" p2="xocar" p3="encarar"/>
  
  <!-- accertare > constatar, assegurar -->
  <macro name="verb_nopron_pron" p1="accertare" p2="constatar" p3="assegurar"/>
  
  <!-- sollevare > plantejar, aixecar -->
  <macro name="verb_nopron_pron" p1="sollevare" p2="plantejar" p3="aixecar"/>
  
  <!-- accomodare > arranjar, acomodar -->
  <macro name="verb_nopron_pron" p1="accomodare" p2="arranjar" p3="acomodar"/>
  
  <!-- guardare > mirar, guardar -->
  <macro name="verb_nopron_pron" p1="guardare" p2="mirar" p3="guardar"/>
  
  <!-- trasferire > transferir, traslladar -->
  <macro name="verb_nopron_pron" p1="trasferire" p2="transferir" p3="traslladar"/>
  
  <!-- fare# venire > fer# venir, fer# portar -->
  <macro name="verb_nopron_pron" p1="fare# venire" p2="fer# venir" p3="fer# portar"/>
  
  <!-- levare > llevar, alçar -->
  <macro name="verb_nopron_pron" p1="levare" p2="llevar" p3="alçar"/>
  
  <!-- affrontare > afrontar, enfrontar -->
  <macro name="verb_nopron_pron" p1="affrontare" p2="afrontar" p3="enfrontar"/>
  
  <!-- diplomare > diplomar, graduar -->
  <macro name="verb_nopron_pron" p1="diplomare" p2="diplomar" p3="graduar"/>
  
  <!-- intendere > tenir# la intenció, tenir# la intenció de, entendre -->
  <macro name="verb_nopron_pron" p1="intendere" p2="tenir# la intenció" p3="entendre"/>
  <rule weight="1.2">
    <match lemma="intendere" tags="vblex.*"><select lemma="tenir# la intenció de" tags="vblex.*"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="vblex.inf"/>
  </rule>

  <!-- rovesciare > tombar, derrocar, bolcar, capgirar -->
  <macro name="verb_nopron_pron" p1="rovesciare" p2="tombar" p3="capgirar"/>

  <!-- diventare > esdevenir, tornar -->
  <macro name="verb_nopron_pron" p1="diventare" p2="esdevenir" p3="tornar"/>

  <!-- chiamare > cridar, dir -->
  <macro name="verb_nopron_pron" p1="chiamare" p2="cridar" p3="dir"/>
  <rule weight="1.0">
    <match lemma="chiamare" tags="vblex.pp.*"><select lemma="anomenar"/></match>
<!--
    <or>
      <match tags="n.*"/>
      <match tags="apos"/>
    </or>
-->
  </rule>
  <rule weight="1.0">
    <match lemma="lo" tags="prn.pro.p3.*"/>
    <match lemma="chiamare" tags="vblex.*"><select lemma="anomenar"/></match>
    <or>
      <match tags="n.*"/>
      <match tags="apos"/>
    </or>
  </rule>

  <!-- cercare > buscar, cercar, intentar -->
  <rule weight="0.6">
    <match lemma="cercare" tags="vblex.*"><select lemma="buscar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cercare" tags="vblex.*"><select lemma="intentar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="di" tags="pr"/>
  </rule>

  <!-- tentare > temptar, intentar -->
  <rule weight="0.6">
    <match lemma="tentare" tags="vblex.*"><select lemma="temptar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tentare" tags="vblex.*"><select lemma="tractar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="di" tags="pr"/>
  </rule>
  
  <!-- essere > ser, estar -->
  <rule weight="0.6">
    <match lemma="essere" tags="vbser.*"><select lemma="ser"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="essere" tags="vbser.*"><select lemma="estar"/></match>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <or>
      <match lemma="situare" tags="vblex.pp.*"/>
      <match lemma="dividere" tags="vblex.pp.*"/>
      <match lemma="suddividere" tags="vblex.pp.*"/>
      <match lemma="sposare" tags="vblex.pp.*"/>
      <match lemma="comporre" tags="vblex.pp.*"/>
      <match lemma="decorare" tags="vblex.pp.*"/>
      <match lemma="ispirare" tags="vblex.pp.*"/>
      <match lemma="abituare" tags="vblex.pp.*"/>
      <match lemma="abituato" tags="adj.*"/>
      <match lemma="pronto" tags="adj.*"/>
      <match lemma="preparare" tags="vblex.pp.*"/>
      <match lemma="preparato" tags="adj.*"/>
    </or>
  </rule>

<!-- **************** REL **************** -->

  <!-- che > que, què, qual -->
  <rule weight="0.6">
     <match lemma="che" tags="rel.*"><select lemma="que"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="pr"/>
     <match lemma="che" tags="rel.*"><select lemma="què"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="det.*"/>
     <match lemma="che" tags="rel.*"><select lemma="qual"/></match>
  </rule>
  <rule weight="1.2">
     <match lemma="quello" tags="det.*"/>
     <match lemma="che" tags="rel.*"><select lemma="que"/></match>
  </rule>

  <!-- cui > que, què, qual -->
  <rule weight="0.6">
     <match lemma="cui" tags="rel.*"><select lemma="a qui"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="pr"/>
     <match lemma="cui" tags="rel.*"><select lemma="què"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="det.*"/>
     <match lemma="cui" tags="rel.*"><select lemma="qual"/></match>
  </rule>

  <!-- quale > com, qual -->
  <rule weight="0.6">
     <match lemma="quale" tags="rel.*"><select lemma="com"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="det.def.*"/>
     <match lemma="quale" tags="rel.*"><select lemma="qual"/></match>
  </rule>

<!-- **************** PR **************** -->

  <!-- di > de, che (comparacions) -->
  <rule weight="0.6">
    <match lemma="di" tags="pr"><select lemma="de"/></match>
  </rule>

  <!-- fino > fins, fins a -->
  <rule weight="0.8">
    <match lemma="fino" tags="pr"><select lemma="fins"/></match>
  </rule>

  <!-- fino a > fins a, fins -->
  <rule weight="0.8">
    <match lemma="fino a" tags="pr"><select lemma="fins a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="fino a" tags="pr"><select lemma="fins"/></match>
    <or>
      <match tags="num"/>
      <match tags="num.*"/>
      <match tags="det.*"/>
      <match tags="adj.*"/>
    </or>
    <match tags="n.*"/>
    <match lemma="fare" tags="vblex.pri.p3.sg"/>
  </rule>

  <!-- per > per, per a, a punt de -->
  <rule weight="0.6">
    <match lemma="per" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="stare" tags="vblex.*"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="per" tags="pr"><select lemma="a punt de"/></match>
    <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>

<!-- regles en el cas d'un possible canvi a "per a" com a traducció per defecte
  <rule weight="1.0">
    <match lemma="per" tags="pr"><select lemma="per"/></match>
    <repeat from="0" upto="1">
