      <cat-item lemma="n_hi_ha" tags="SV.*"/>
    </def-cat>

    <def-cat n="avere_bisogno">
      <cat-item lemma="avere# bisogno" tags="SV.vblex.*"/>
    </def-cat>

    <def-cat n="verbs_de">
      <cat-item lemma="cercare" tags="SV.*"/>
      <cat-item lemma="tentare" tags="SV.*"/>
