<rule comment="REGLA: SV TOP_potser_det > SV il TOP_potser_det">
    <pattern>
        <pattern-item n="SV_Cnj_Rel_Cm"/>
        <pattern-item n="top_potser_det"/>
    </pattern>
    <action>
      <out>
        <chunk>
          <clip pos="1" part="lem"/>
          <clip pos="1" part="tags"/>
          <clip pos="1" part="chcontent"/>
        </chunk>
	<b pos="1"/>
        <chunk>
          <lit v="il"/>
          <lit-tag v="SA"/>
          <clip pos="2" part="gen"/>
          <clip pos="2" part="nbr"/>
          <lit v="{^"/>
          <lit v="il"/>
          <lit-tag v="det.def.2.3"/>
          <lit v="$}"/>
        </chunk>
        <b/>
        <chunk>
          <clip pos="2" part="lem"/>
          <clip pos="2" part="tags"/>
          <clip pos="2" part="chcontent"/>
        </chunk>
      </out>
</action>
</rule>

<rule comment="REGLA: sent TOP_potser_det > sent Il TOP_potser_det">
    <pattern>
        <pattern-item n="sent"/>
        <pattern-item n="top_potser_det"/>
    </pattern>
    <action>
      <out>
        <chunk>
          <clip pos="1" part="lem"/>
          <clip pos="1" part="tags"/>
          <clip pos="1" part="chcontent"/>
        </chunk>
	<b pos="1"/>
      </out>
      <choose>
        <when>
          <test>
            <equal><clip pos="1" part="lem"/><lit v=":"/></equal>
          </test>
          <out>
            <chunk>
              <lit v="il"/>
              <lit-tag v="SA"/>
              <clip pos="2" part="gen"/>
              <clip pos="2" part="nbr"/>
              <lit v="{^"/>
              <lit v="il"/>
              <lit-tag v="det.def.2.3"/>
              <lit v="$}"/>
            </chunk>
          </out>
        </when>
        <otherwise>
          <out>
            <chunk>
              <lit v="Il"/>
              <lit-tag v="SA"/>
              <clip pos="2" part="gen"/>
              <clip pos="2" part="nbr"/>
              <lit v="{^"/>
              <lit v="Il"/>
              <lit-tag v="det.def.2.3"/>
              <lit v="$}"/>
            </chunk>
          </out>
        </otherwise>
      </choose>
      <out>
        <b/>
        <chunk>
          <clip pos="2" part="lem"/>
          <clip pos="2" part="tags"/>
          <clip pos="2" part="chcontent"/>
        </chunk>
      </out>
</action>
</rule>

