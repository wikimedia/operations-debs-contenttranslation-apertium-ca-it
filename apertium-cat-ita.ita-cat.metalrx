-->

  <!-- come > com, com a -->
  <rule weight="0.6">
    <match lemma="come" tags="pr"><select lemma="com"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="come" tags="pr"><select lemma="com a"/></match>
    <match tags="n.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="come" tags="pr"><select lemma="com"/></match>
    <match lemma="dio" tags="n.*"/>
  </rule>

  <!-- oltre > més enllà de, més de -->
  <rule weight="0.6">
    <match lemma="oltre" tags="pr"><select lemma="més enllà de"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="oltre" tags="pr"><select lemma="més de"/></match>
    <or><match tags="num"/><match tags="num.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="oltre" tags="pr"><select lemma="més de"/></match>
    <match tags="det.def.*"/>
    <or>
      <seq n="periodo"/>
    </or>
  </rule>

  <!-- da > de, per, des de -->
  <rule weight="0.6">
    <match lemma="da" tags="pr"><select lemma="de"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="molto" tags="adv"/>
    <match lemma="da" tags="pr"><select lemma="a" tags="pr"/></match>
     <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>
<!-- VERIFY: regla dubtosa
  <rule weight="1.0">
    <match lemma="da" tags="pr"><select lemma="per"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <or><match tags="vblex.inf"/><match tags="vbmod.inf"/><match tags="vbhaver.inf"/><match tags="vbser.inf"/></or>
  </rule>
-->
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="da" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="in" tags="pr"/>
    <match lemma="il" tags="det.def.*"/>
    <match tags="num"/>
    <match lemma="da" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="il" tags="det.def.*"/>
    <match tags="num"/>
    <or>
      <seq n="mese"/>
    </or>
    <match lemma="da" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="il" tags="det.def.*"/>
    <match tags="num"/>
    <or>
      <seq n="mese"/>
    </or>
    <match tags="num"/>
    <match lemma="da" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="da" tags="pr"><select lemma="des de"/></match>
    <match tags="det.def.m.sg"/>
    <match tags="num"/>
  </rule>
  <rule weight="1.0">
    <match lemma="attraversare" tags="vblex.*"/>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="da" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="1.2">
    <or>
      <seq n="verbi_da"/>
    </or>
    <repeat from="0" upto="1">
      <or>
        <match tags="prn.pro.*"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <match lemma="da" tags="pr"><select lemma="de"/></match>
  </rule>

  <!-- tra, fra > entre, a -->
  <rule weight="0.6">
    <or>
      <match lemma="tra" tags="pr"><select lemma="entre"/></match>
      <match lemma="fra" tags="pr"><select lemma="entre"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="tra" tags="pr"><select lemma="a"/></match>
      <match lemma="fra" tags="pr"><select lemma="a"/></match>
    </or>
    <or><match tags="num.*"/><match tags="det.ind.f.sg"/></or>
    <or>
      <seq n="distanza"/>
    </or>
  </rule>

  <!-- a > a, fins, de, en -->
  <rule weight="0.6">
    <match lemma="a" tags="pr"><select lemma="a"/></match>
